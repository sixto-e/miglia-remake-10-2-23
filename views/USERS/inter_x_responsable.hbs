<div class="container-fluid">
  <div class="row">
    <div class="col-md-2 sinpadding">
      {{> lateralBeta}}
    </div>
    <!--DENTRO DE ESTE DIV VA: la cabecera y el contenido-->
    <div class="col-md-10 sinpadding">
      {{> cabeceraBeta}}<!--cabecera-->
      <div class="row box-ind sinpadding"><!-- Dentro de este div va todo el contenido-->

        <div class="col-md-12 box-cajas">
          {{#each filtro}}
          <div class="container-fluid espacio-bloque">
            <div class="row">
              <div class="col-md-1 fondo-color-dorado-icono">
                <i class='bx bxs-contact'></i>
              </div>
              <div class="col-md-11 fondo-color-dorado-contenido">
                <div class="row">

                  <div class="col-md-4 propiedad-columna-bloque-txt">
                    <div>
                      <strong>Responsable de seguimiento:</strong> {{seguimiento}}
                    </div>

                    <div>
                      <strong>Estado interacción: </strong> {{estado_interaccion}}
                    </div>

                    <div>
                      <strong>Próximo contacto: </strong> {{"DATE_FORMAT(inte.proximo_contacto, '%d-%m-%Y')"}}
                    </div>

                  </div>

                  <div class="col-md-4 propiedad-columna-bloque-txt">
                    <div>
                      <strong>Razón social:</strong> {{razon_social}}
                    </div>

                    <div>
                      <strong>Teléfono: </strong> {{telefono}}
                    </div>

                    <div>
                      <strong>Interés en: </strong> {{interesado_en}}
                    </div>
                  </div>

                  <div class="col-md-4 propiedad-columna-bloque-boton">

                    <div class="dropdown">
                      <button class="btn-sm btn btn-outline-dark dropdown-toggle" 
                        type="button" data-bs-toggle="dropdown" aria-expanded="false">
                        Acciones
                      </button>
                      <ul class="dropdown-menu">
                        <li><a class="dropdown-item" href="/interacciones/{{id}}">ver</a></li>
                        <li><a class="dropdown-item" href="/interacciones/borrar/{{id}}"></a></li>
                      
                      </ul>
                    </div>

                  </div><!--Findde Col md3 -->

                </div>
              </div>
            </div>
          </div>
          {{/each}}
        </div><!--fin col-md-12 box-cajas-->

      </div><!--fin row box-ind-->

    </div><!--fin col-md-10-->

  </div><!--fin row-->

  <div><!--fin container fluid-->
    <!---->